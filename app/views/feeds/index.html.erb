<h1>Listing feeds</h1>

<div id="feed-form">
  <%= link_to 'New feed', new_feed_path, remote: true %>
</div>

<div class="container-fluid">
  <div class="row" id="feeds_container">
    <% @feeds.each do |feed| %>
      <div id="feed_<%= feed.id %>" class="col-lg-4">
        <%= render partial: 'feeds/feed', locals: {feed: feed} %>
      </div>
    <% end %>
  </div>
</div>